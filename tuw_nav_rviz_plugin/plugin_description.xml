<library path="lib/libtuw_rviz_plugin">
  <class name="tuw_nav_rviz_plugin/Spline"
         type="tuw_nav_rviz_plugin::SplineDisplay"
         base_class_type="rviz::Display">
    <description>
      Displays a B-Spline (up to 3d)
    </description>
    <message_type>tuw_nav_msgs/Spline</message_type>
  </class>
  <class name="tuw_nav_rviz_plugin/RouteSegments"
         type="tuw_nav_rviz_plugin::RouteSegmentsDisplay"
         base_class_type="rviz::Display">
    <description>
      Displays RouteSegments
    </description>
    <message_type>tuw_nav_msgs/RouteSegments</message_type>
  </class>
</library>

